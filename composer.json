{
    "name": "cristianhr/json-mini-bread",
    "description": "Creates a mini BREAD nested inside another model.",
    "license": "MIT",
    "authors": [
        {
            "name": "Cristian Hilario Rentera",
            "email": "khristiamr@gmail.com"
        }
    ],
    "require": {
        "larapack/hooks": "1.*",
        "tcg/voyager": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Cristianhr\\JsonMiniBreadHook\\": "src/"
        }
    },
    "extra": {
        "hook": {
            "providers": [
                "Cristianhr\\JsonMiniBreadHook\\JsonMiniBreadHookServiceProvider"
            ],
            "aliases": {
                "JsonMiniBreadHook": "Cristianhr\\JsonMiniBreadHook\\Facades\\JsonMiniBreadHookFacade"
            },
            "views": [
                "resources/views"
            ],
            "lang": [
                "resources/lang"
            ]
        }
    }
}
